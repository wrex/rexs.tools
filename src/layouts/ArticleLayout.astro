---
import BaseLayout from "./BaseLayout.astro";
const { frontmatter } = Astro.props;
---

<BaseLayout
  title={frontmatter.title}
  description={frontmatter.description}
  permalink={frontmatter.permalink}
  current="articles"
>
  <article class="flow">
    <h1>{frontmatter.title}</h1>
    <slot />
  </article>
</BaseLayout>
